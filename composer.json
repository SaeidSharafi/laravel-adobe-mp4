{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "abbasudo/pulse-users": "^0.4.0",
        "based/momentum-modal": "^0.2.0",
        "doctrine/dbal": "^3.0",
        "fakerphp/faker": "^1.9.1",
        "guzzlehttp/guzzle": "^7.5",
        "hekmatinasser/verta": "^8.3",
        "hosmelq/laravel-pulse-schedule": "^0.1.0",
        "inertiajs/inertia-laravel": "^0.6.8",
        "inspector-apm/inspector-laravel": "^4.7",
        "laravel/framework": "^10.0",
        "laravel/pulse": "^1.0@beta",
        "laravel/sanctum": "^3.2",
        "laravel/slack-notification-channel": "^2.5",
        "laravel/telescope": "^4.14",
        "laravel/tinker": "^2.8.1",
        "maantje/pulse-database": "^0.1.0",
        "maatwebsite/excel": "^3.1",
        "ohdearapp/ohdear-pulse": "*",
        "opcodesio/log-viewer": "^2.4",
        "paulo-hortelan/requests-graph-pulse": "^1.0",
        "saeidsharafi/laravel-sms": "^1.0",
        "spatie/laravel-activitylog": "^4.7",
        "spatie/laravel-backup": "^8.1.7",
        "spatie/laravel-permission": "^5.10.0",
        "spatie/laravel-query-builder": "^5.2.0",
        "staudenmeir/belongs-to-through": "^2.13",
        "tightenco/ziggy": "^1.0"
    },
    "require-dev": {
        "aschmelyun/fleet": "^1.1",
        "barryvdh/laravel-debugbar": "^3.8",
        "brianium/paratest": "^7.1.0",
        "larastan/larastan": "^2.8",
        "laravel/breeze": "^1.19.2",
        "laravel/pint": "^1.7",
        "laravel/sail": "^1.21.3",
        "mockery/mockery": "^1.5.1",
        "nunomaduro/collision": "^7.0",
        "pestphp/pest": "^2.4",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpunit/phpunit": "^10.0",
        "roave/security-advisories": "dev-latest",
        "saeidsharafi/laravel-vue-i18n-generator": "^0.1",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "beta",
    "prefer-stable": true
}
